    networks:
      default: null
    ports:
    - mode: ingress
      target: 1433
      published: "8002"
      protocol: tcp
    secrets:
    - source: db_password
  api:
    build:
      context: C:\Users\dell\source\repos\TodoAppBackend
      dockerfile: API/Dockerfile
